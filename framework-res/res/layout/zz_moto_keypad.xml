        <View style="@style/hDivSpaceDialPad" />
        <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="0.0px" android:layout_weight="1.0">
            <include android:id="@id/four" layout="@layout/zz_moto_button_keypad_1" />
            <View style="@style/vDivSpaceDialPad" />
            <include android:id="@id/five" layout="@layout/zz_moto_button_keypad_1" />
            <View style="@style/vDivSpaceDialPad" />
