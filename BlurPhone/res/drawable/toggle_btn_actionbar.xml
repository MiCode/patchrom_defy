<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:drawable="@drawable/btn_action_bar_checked" />
    <item android:state_window_focused="false" android:state_enabled="true" android:drawable="@blur.res:drawable/btn_action_bar_normal" />
    <item android:state_window_focused="false" android:state_enabled="false" android:drawable="@blur.res:drawable/btn_action_bar_normal_disable" />
    <item android:state_pressed="true" android:drawable="@blur.res:drawable/btn_action_bar_pressed" />
    <item android:state_focused="true" android:state_enabled="true" android:drawable="@blur.res:drawable/btn_action_bar_selected" />
    <item android:state_enabled="true" android:drawable="@blur.res:drawable/btn_action_bar_normal" />
    <item android:state_focused="true" android:drawable="@blur.res:drawable/btn_action_bar_normal_disable_focused" />
    <item android:drawable="@blur.res:drawable/btn_action_bar_normal_disable" />
</selector>
